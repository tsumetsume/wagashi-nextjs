{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "migrate:supabase": "tsx scripts/migrate-to-supabase.ts",
    "types:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > lib/database.types.ts",
    "db:test": "tsx scripts/test-db-connection.ts",
    "dev:local": "next dev",
    "db:local:setup": "pnpm db:generate && pnpm db:push && pnpm db:seed",
    "db:local:reset": "pnpm db:push --force-reset && pnpm db:seed",
    "storage:local:clean": "rm -rf public/uploads/products/* && echo 'ローカル画像ストレージをクリーンアップしました'",
    "test:e2e": "playwright test",
    "test:e2e:setup": "./scripts/reset-test-db.sh && playwright test",
    "test:e2e:docker": "playwright test --config=playwright.docker.config.ts",
    "test:e2e:docker:setup": "./scripts/reset-test-db.sh && playwright test --config=playwright.docker.config.ts",
    "db:reset:test": "./scripts/reset-test-db.sh"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
